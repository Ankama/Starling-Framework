<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">    <modelVersion>4.0.0</modelVersion>        <parent>        <groupId>com.ankama.dofus</groupId>        <artifactId>dofus-team</artifactId>        <version>1.0</version>    </parent>        <groupId>com.gamua</groupId>    <artifactId>starling</artifactId>    <version>2.4-dofus</version>    <packaging>swc</packaging>        <properties>        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>        <targetDirectory>target</targetDirectory>        <sourceDirectory>starling/src</sourceDirectory>        <flex.version>4.6.0</flex.version>        <air.version>29.0</air.version>        <flexmojos.version>7.1.1</flexmojos.version>        <flex.home>${settings.localRepository}/org/apache/flex/framework</flex.home>    </properties>        <build>        <sourceDirectory>${sourceDirectory}</sourceDirectory>        <directory>${targetDirectory}</directory>        <plugins>            <plugin>                <groupId>net.flexmojos.oss</groupId>                <artifactId>flexmojos-maven-plugin</artifactId>                <version>${flexmojos.version}</version>                <extensions>true</extensions>                <dependencies>                    <dependency>                        <groupId>org.apache.flex</groupId>                        <artifactId>compiler</artifactId>                        <version>${flex.version}</version>                        <type>pom</type>                    </dependency>                    <dependency>                        <groupId>com.adobe.air</groupId>                        <artifactId>compiler</artifactId>                        <version>${air.version}</version>                        <type>pom</type>                    </dependency>                </dependencies>                <configuration>                    <airVersion>${air.version}</airVersion>                    <compilerWarnings>                        <warn-no-constructor>false</warn-no-constructor>                    </compilerWarnings>                    <themes>                        <theme>${flex.home}/themes/spark/${flex.version}/spark-${flex.version}.swc</theme>                    </themes>                </configuration>            </plugin>            <plugin>                <artifactId>maven-source-plugin</artifactId>                <version>3.0.1</version>                <executions>                    <execution>                        <id>attach-sources</id>                        <goals>                            <goal>jar-no-fork</goal>                        </goals>                    </execution>                </executions>            </plugin>            <plugin>                <artifactId>maven-install-plugin</artifactId>                <version>2.5.2</version>            </plugin>            <plugin>                <artifactId>maven-deploy-plugin</artifactId>                <version>2.8.2</version>            </plugin>        </plugins>    </build>        <dependencies>        <dependency>            <groupId>com.adobe.air</groupId>            <artifactId>framework</artifactId>            <version>${air.version}</version>            <type>pom</type>        </dependency>        <dependency>            <groupId>org.apache.flex</groupId>            <artifactId>framework</artifactId>            <version>${flex.version}</version>            <type>pom</type>        </dependency>        <dependency>            <groupId>org.apache.flex.framework.themes</groupId>            <artifactId>spark</artifactId>            <version>${flex.version}</version>            <type>swc</type>            <scope>runtime</scope>        </dependency>    </dependencies></project>